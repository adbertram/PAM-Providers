{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListIAMUsers",
            "Effect": "Allow",
            "Action": [
                "iam:ListUsers",
                "iam:GetUser"
            ],
            "Resource": "*"
        },
        {
            "Sid": "ChangeIAMUserPasswords",
            "Effect": "Allow",
            "Action": [
                "iam:UpdateLoginProfile"
            ],
            "Resource": "arn:aws:iam::*:user/*"
        }
    ]
}