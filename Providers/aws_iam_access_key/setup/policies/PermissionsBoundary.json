{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "PreventPrivilegeEscalation",
            "Effect": "Deny",
            "Action": [
                "iam:Create*",
                "iam:Delete*",
                "iam:Attach*",
                "iam:Detach*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "ListEntities",
            "Effect": "Allow",
            "Action": [
                "iam:ListAccessKeys",
                "iam:ListUsers"
            ],
            "Resource": "*"
        }
    ]
}